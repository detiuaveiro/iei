[Application]
name=pt.ua.deti.iei.tictactoe
runtime=org.gnome.Platform/x86_64/48
sdk=org.gnome.Sdk/x86_64/48
command=game

[Context]
shared=ipc;
sockets=x11;wayland;
devices=dri;

[Environment]
PYTHONPATH=/app/lib/game

[Extension pt.ua.deti.iei.tictactoe.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=pt.ua.deti.iei.tictactoe.Debug;